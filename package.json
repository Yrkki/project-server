{
  "name": "project-server",
  "version": "2.0.0",
  "description": "CV Generator Project Server",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "test": "echo npx snyk test",
    "snyk-protect": "echo npx snyk-protect",
    "prepare": "npm run snyk-protect",
    "dev:plan:update:report:action:update-packages": "npm-run-all update-packages:*",
    "update-packages:install": "npm install --legacy-peer-deps",
    "update-packages:npx-npm-check-updates-u": "npx npm-check-updates --timeout 600000 -u --packageFile package.json && npm install --legacy-peer-deps"
  },
  "keywords": [
    "CV Generator",
    "ProjectServer"
  ],
  "author": "jorich",
  "license": "Apache-2.0",
  "dependencies": {
    "@snyk/protect": "^1.951.0",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.6",
    "cors": "^2.8.5",
    "debug": "~4.3.4",
    "express": "~4.18.1",
    "mime-types": "^2.1.35",
    "morgan": "~1.10.0",
    "ms": "^2.1.3",
    "nconf": "^0.12.0",
    "pug": "^3.0.2",
    "serve-favicon": "~2.5.0",
    "spdy": "^4.0.2",
    "url-exists": "^1.0.3"
  },
  "engines": {
    "node": "<17"
  },
  "snyk": true,
  "devDependencies": {
    "typescript": "^4.7.3"
  }
}
